<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - App.Web.Mvc</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AspNet_MVC_Cms.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/template/plugins/icofont/icofont.min.css">
    <link rel="stylesheet" href="~/template/plugins/slick-carousel/slick/slick.css">
    <link rel="stylesheet" href="~/template/plugins/slick-carousel/slick/slick-theme.css">
    <link rel="stylesheet" href="~/template/css/style.css">

    <link rel="shortcut icon" type="image/x-icon" href="~/template/images/favicon.png" />
 
</head>
<body>
   
    <div class="container">
        <main role="main" class="pb-3">
            <partial name="_TopBar" />
            <partial name="_Header" />

            @RenderBody()
            <partial name="_Footer" />
        </main>
    </div>
    
    
    <!-- Google Map -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkeLMlsiwzp6b3Gnaxd86lvakimwGA6UA"></script>
    <script src="~/template/plugins/google-map/gmap.js"></script>

   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script src="~/template/plugins/jquery/jquery.js"></script>
    <script src="~/template/plugins/bootstrap/bootstrap.min.js"></script>
    <script src="~/template/plugins/slick-carousel/slick/slick.min.js"></script>
    <script src="~/template/plugins/shuffle/shuffle.min.js"></script>

   
    <script src="~/template/js/script.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
